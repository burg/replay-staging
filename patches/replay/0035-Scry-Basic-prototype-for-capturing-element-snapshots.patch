+ElementSnapshot::ElementSnapshot(std::unique_ptr<ImageBuffer> data, const LayoutRect& bounds, uint64_t snapshotId)
+    : imageData(std::move(data))
+    , boundingBox(bounds)
+    , snapshotId(snapshotId)
+{
